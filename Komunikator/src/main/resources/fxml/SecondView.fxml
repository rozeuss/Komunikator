<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="container" prefHeight="693.0" prefWidth="774.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextArea fx:id="conversationTextArea" editable="false" prefHeight="354.0" prefWidth="734.0" wrapText="true">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextArea>
      <TextField fx:id="chatTextField" onKeyReleased="#chatTFonKeyReleased" prefHeight="0.0" prefWidth="734.0" />
      <Button fx:id="sendMessageButton" mnemonicParsing="false" onAction="#sendMessageButtonOnAction" text="Send" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" />
   </padding>
</VBox>
